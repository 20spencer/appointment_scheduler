<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="220.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduler.LoginController">
   <children>
      <TextField fx:id="loginUsernameTxt" layoutX="20.0" layoutY="54.0" prefHeight="35.0" prefWidth="160.0" promptText="User ID" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="55.0" />
      <PasswordField fx:id="loginPasswordTxt" layoutX="20.0" layoutY="99.0" prefHeight="35.0" prefWidth="160.0" promptText="Password" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="100.0" />
      <Button fx:id="loginConfirmBtn" alignment="CENTER" contentDisplay="CENTER" layoutX="171.0" layoutY="145.0" mnemonicParsing="false" onAction="#loginConfirmBtnAction" text="Confirm" textAlignment="CENTER" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="145.0" />
      <Label fx:id="lblLoginHeader" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="14.0" prefHeight="27.0" prefWidth="200.0" text="LOGIN" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="14.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <HBox layoutY="203.0" prefHeight="15.0" prefWidth="200.0" AnchorPane.bottomAnchor="2.0" AnchorPane.leftAnchor="0.0">
         <children>
            <Label fx:id="loginLocationLbl" text="Location: ">
               <font>
                  <Font size="10.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lblLocationToSet">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
